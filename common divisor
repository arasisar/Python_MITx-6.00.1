def gcdIter(a, b):
    '''
    a, b: positive integers
    
    returns: a positive integer, the greatest common divisor of a & b.
    '''
    # Your code here
    m = min(a, b)

    # Keep looping until testValue divides both a & b evenly
    while a % m != 0 or b % m != 0:
        m -= 1

    return m
